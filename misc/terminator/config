  next_tab = <Super>period
  prev_tab = <Super>comma
  go_down = <Shift><Alt>j
  go_right = <Shift><Alt>l
  go_left = <Shift><Alt>h
[profiles]
  [[default]]
    use_system_font = False
    background_darkness = 0.88
    background_type = transparent
    scrollbar_position = hidden
    icon_bell = False
    show_titlebar = False
